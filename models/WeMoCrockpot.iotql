--
--  compile to JSON with 'iotql-model WeMoCrockpot.iotql'
--
CREATE MODEL WeMoCrockpot WITH
    schema:name = "WeMo Crockpot",
    schema:description = "Belkin WeMo Crockpot",
    iot:model = "https://raw.githubusercontent.com/dpjanes/homestar-wemo/master/models/WeMoCrockpot.json"
ATTRIBUTE on WITH
    schema:name = "on",
    iot:purpose = iot-purpose:on,
    iot:type = iot:type.boolean,
    iot:actuator = true,
    iot:sensor = true,
    iot:write = true,
    iot:read = true
;
